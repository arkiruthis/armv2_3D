# Project:   Projects


# Toolflags:
CCflags = -c -depend !Depend -IC: -throwback
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -aif -c++ -o $@
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags = 
LibFileflags = -c -o $@
Squeezeflags = -o $@


# Final targets:
@.!MeshRenderer:   @.o.Main @.o.Math3D @.o.Mesh @.o.Utils \
        @.^.AcornC_C++.Libraries.clib.o.ansilib @.^.AcornC_C++.Libraries.clib.o.stubs 
        Link $(Linkflags) @.o.Main @.o.Math3D @.o.Mesh @.o.Utils \
        @.^.AcornC_C++.Libraries.clib.o.ansilib @.^.AcornC_C++.Libraries.clib.o.stubs 


# User-editable dependencies:


# Static dependencies:
@.o.Main:   @.c.Main
        cc $(ccflags) -o @.o.Main @.c.Main 
@.o.Math3D:   @.c.Math3D
        cc $(ccflags) -o @.o.Math3D @.c.Math3D 
@.o.Mesh:   @.c.Mesh
        cc $(ccflags) -o @.o.Mesh @.c.Mesh 
@.o.Utils:   @.s.Utils
        objasm $(objasmflags) -from @.s.Utils -to @.o.Utils


# Dynamic dependencies:
o.Utils: s.Utils
o.Utils: h.SWINames
o.Utils: s.Utils
o.Utils: h.SWINames
o.Main:	c.Main
o.Main:	C:h.kernel
o.Main:	C:h.swis
o.Main:	h.Math3D
o.Main:	h.Mesh
o.Main:	h.Math3D
o.Main:	h.CVector
o.Main:	h.CVector
o.Main:	c.Main
o.Main:	C:h.kernel
o.Main:	C:h.swis
o.Main:	h.Math3D
