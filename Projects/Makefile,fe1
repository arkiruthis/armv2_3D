# Project:   MeshRenderer


# Toolflags:
CCflags = -c -depend !Depend -IC: -throwback 
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -aif -c++ -o $@
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags = 
LibFileflags = -c -o $@
Squeezeflags = -o $@


# Final targets:
@.!MeshRenderer:   @.o.Utils @.o.Main @.^.AcornC_C++.Libraries.clib.o.stubs \
        @.o.Math3D @.o.Mesh @.^.AcornC_C++.Libraries.clib.o.ansilib @.o.Palette 
        Link $(Linkflags) @.o.Utils @.o.Main \
        @.^.AcornC_C++.Libraries.clib.o.stubs @.o.Math3D @.o.Mesh @.^.AcornC_C++.Libraries.clib.o.ansilib @.o.Palette 


# User-editable dependencies:


# Static dependencies:
@.o.Main:   @.c.Main
        cc $(ccflags) -o @.o.Main @.c.Main 
@.o.Utils:   @.s.Utils
        objasm $(objasmflags) -from @.s.Utils -to @.o.Utils
@.o.Math3D:   @.c.Math3D
        cc $(ccflags) -o @.o.Math3D @.c.Math3D 
@.o.Mesh:   @.c.Mesh
        cc $(ccflags) -o @.o.Mesh @.c.Mesh 
@.o.Palette:   @.c.Palette
        cc $(ccflags) -o @.o.Palette @.c.Palette 


